{
  "expo": {
    "name": "OCD Journey",
    "slug": "ocd-tracker",
    "version": "1.0.2",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "orientation": "portrait",
    "icon": "./assets/images/ios-light.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/fonts",
      "assets/images",
      "assets/sounds"
    ],
    "description": "Track your OCD journey by recording a daily journal and leveling up your ability to resist compulsions.",
    "ios": {
      "bundleIdentifier": "com.twigs.ocdtracker",
      "buildNumber": "13",
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#EFE9E7"
      },
      "package": "com.twigs.ocdtracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#EFE9E7",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#353249"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "ocd-journey"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c1aa15b-ade6-4978-a2f2-ac336685123d"
      }
    }
  }
}
